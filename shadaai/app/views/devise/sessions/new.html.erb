<div class="container-fluid" style="margin-top:10%; margin-bottom:15%; margin-left:14%;">
  <!-- <h2>Iniciar sesión</h2> -->
  <div class="row">
    <div class="col-xs-5 col-sm-5 col-md-5">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="form-group">
            <%= f.label :email, "Correo electrónico:" %><br />
            <%= f.email_field :email, autofocus: true, class:"form-control" %>
          </div>

          <div class="form-group">
            <%= f.label :password, "Contraseña:" %><br />
            <%= f.password_field :password, autocomplete: "off", class:"form-control" %>
          </div>

          <% if devise_mapping.rememberable? -%>
            <div class="form-group">
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me, "Recuérdame" %>
            </div>
          <% end -%>

          <div class="actions">
            <%= f.submit "Ingresar", class:"btn btn-primary" %>
          </div>
        <% end %>

        <div style="margin-top:1em">
          <%= render "devise/shared/links" %>
        </div>
    </div>

    <div class="col-xs-1 col-sm-1 col-md-1" style="margin-top:4%;">
      <%= image_tag "footer.png", class: 'img-responsive'%>
    </div>

    <div class="col-xs-5 col-sm-5 col-md-5">
      <%= image_tag "medico1.png", class: 'img-responsive'%>   
    </div>
  </div>
</div>

